{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "A simple Minecraft client-side mod, that reverts the `run_command` click event restriction introduced in 1.19.1-rc1, so non-command messages can be sent freely via `run_command` click event again",
  "authors": [
    "Fallen_Breath"
  ],
  "contact": {
    "homepage": "https://github.com/Fallen-Breath/LetMeClickAndSend",
    "sources": "https://github.com/Fallen-Breath/LetMeClickAndSend"
  },
  "icon": "assets/letmeclickandsend/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "me.fallenbreath.letmeclickandsend.LetMeClickAndSendMod"
    ]
  },
  "mixins": [
    "letmeclickandsend.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}"
  }
}
