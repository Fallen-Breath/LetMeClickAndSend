{
  "schemaVersion": 1,
  "id": "letmeclickandsend",
  "version": "${version}",
  "name": "Let me click and send",
  "description": "A simple Minecraft mod that reverts click event `run_command` restriction which introduced in 1.19.1-rc1, so non-command messages can be sent freely by a click again",
  "authors": [
    "Fallen_Breath"
  ],
  "contact": {
    "homepage": "https://github.com/Fallen-Breath/LetMeClickAndSend",
    "sources": "https://github.com/Fallen-Breath/LetMeClickAndSend"
  },
  "icon": "assets/letmeclickandsend/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "me.fallenbreath.letmeclickandsend.LetMeClickAndSendMod"
    ]
  },
  "mixins": [
    "letmeclickandsend.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}"
  }
}
